  <script>
	document.addEventListener("DOMContentLoaded", () => {
	  const counters = document.querySelectorAll(".box h3"); // Select all h3 elements
  
	  counters.forEach(counter => {
		const updateCounter = () => {
		  const target = +counter.getAttribute("data-target"); // Get target value
		  const current = +counter.innerText.replace("+", ""); // Remove + for calculation
		  const increment = Math.ceil(target / 150); // Adjust speed by changing 150
  
		  if (current < target) {
			counter.innerText = current + increment; // Update counter
			setTimeout(updateCounter, 15); // Speed of updates
		  } else {
			counter.innerText = target + "+"; // Add + symbol after animation
		  }
		};
  
		updateCounter(); // Start the animation
	  });
	});
  </script>
  

