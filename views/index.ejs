<script>
  document.addEventListener("DOMContentLoaded", () => {
    const brandContainer = document.getElementById("brandContainer");
    const scrollSpeed = 1; // Adjust scroll speed (pixels per frame)

    // Duplicate items for seamless scrolling
    const items = [...brandContainer.children];
    items.forEach(item => {
      const clone = item.cloneNode(true);
      brandContainer.appendChild(clone);
    });

    const startScrolling = () => {
      brandContainer.scrollLeft += scrollSpeed;

      // Reset to start for infinite scrolling
      if (brandContainer.scrollLeft >= brandContainer.scrollWidth / 2) {
        brandContainer.scrollLeft = 0;
      }

      requestAnimationFrame(startScrolling);
    };

    startScrolling();
  });
</script>

