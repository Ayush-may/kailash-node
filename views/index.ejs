<script>
  const galleryContainer = document.querySelector('.gallery-container');
  const galleryItems = document.querySelectorAll('.gallery-item');

  class Carousel {
      constructor(container, items) {
          this.carouselContainer = container;
          this.carouselItems = [...items];
          this.initCarousel();
      }

      initCarousel() {
          this.updateGallery();
          this.autoScroll();
      }

      updateGallery() {
          this.carouselItems.forEach((item) => {
              item.classList.remove(
                  'gallery-item-1',
                  'gallery-item-2',
                  'gallery-item-3',
                  'gallery-item-4',
                  'gallery-item-5',
                  'gallery-item-6'
              );
          });

          this.carouselItems.slice(0, 6).forEach((item, index) => {
              item.classList.add(`gallery-item-${index + 1}`);
          });
      }

      // Function to make the carousel auto-scroll
      autoScroll() {
          setInterval(() => {
              this.carouselItems.push(this.carouselItems.shift()); // Move the first item to the end
              this.updateGallery(); // Update the gallery after each shift
          }, 3000); // Change every 3 seconds
      }
  }

  new Carousel(galleryContainer, galleryItems);
</script>
