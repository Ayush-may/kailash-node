<!-- Visitor Counter -->
<section class="visitor-counter">
  <h2>Total Visitors:</h2>
  <span class="count" id="visitor-count">0</span>
</section>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const visitorCountElement = document.getElementById('visitor-count');
    const finalCount = <%= count %>; // EJS will inject the count as a number here
    let currentCount = 0;

    // Ensure finalCount is a valid number
    if (isNaN(finalCount) || finalCount <= 0) {
      visitorCountElement.textContent = "Error: Invalid visitor count";
      return;
    }

    const interval = setInterval(() => {
      currentCount++;
      visitorCountElement.textContent = currentCount;

      if (currentCount >= finalCount) {
        clearInterval(interval);
      }
    }, 50); // Adjust the interval speed (in milliseconds) as needed
  });
</script>

  
  


